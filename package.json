{
  "name": "cs-crypto",
  "version": "0.5.0",
  "description": "A comprehensive, simple, and secure wrapper around the native WebCrypto API.",
  "main": "lib/",
  "module": "dist/",
  "scripts": {
    "build": "tsc",
    "build-module": "tsc --module esnext --outdir dist",
    "build-all": "npm run build && npm run build-module",
    "build-docs": "typedoc --exclude src/test.ts --excludeNotExported --out docs",
    "build-docs-html": "typedoc --exclude src/test.ts --excludeNotExported --out docs_html --plugin none",
    "view-docs": "find docs_html/index.html || npm run build-docs-html; xdg-open docs_html/index.html &>/dev/null",
    "test": "ava src/test.ts"
  },
  "repository": "git@github.com:very-amused/CS-crypto.git",
  "author": "very-amused <very-amused@pm.me>",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@ava/typescript": "^1.1.1",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "Base64": "^1.1.0",
    "ava": "^3.10.1",
    "eslint": "^7.5.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "node-webcrypto-ossl": "^2.1.0",
    "typedoc": "^0.17.8",
    "typedoc-plugin-markdown": "^2.3.1"
  },
  "dependencies": {
    "typescript": "^3.9.7"
  },
  "ava": {
    "typescript": {
      "rewritePaths": {
        "src/": "lib/"
      }
    }
  }
}
