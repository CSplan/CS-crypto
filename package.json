{
  "name": "cs-crypto",
  "version": "0.8.1",
  "description": "A comprehensive, simple, and secure wrapper around the native WebCrypto API.",
  "type": "module",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "postbuild": "tsc -b src/tsconfig-d.json && rm -rf lib/internal",
    "build:docs": "typedoc src/index.ts --out docs",
    "postbuild:docs": "sed 's/\\[docs\\](docs\\/modules.md)/\\[modules\\](modules.md)/' -i docs/README.md",
    "lint": "eslint src/",
    "pretest": "tsc -b src/tsconfig-test.json",
    "test": "ava src/test.ts",
    "clean": "rm -rf test lib",
    "prepack": "yarn clean && yarn test && yarn build && yarn build:docs"
  },
  "repository": "git+https://github.com/very-amused/cs-crypto",
  "author": "very-amused <very-amused@pm.me>",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@ava/typescript": "^3.0.1",
    "@peculiar/webcrypto": "^1.2.3",
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/node": "^20.6.2",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "ava": "^4.0.1",
    "eslint": "^8.8.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-unicorn": "^48.0.1",
    "rollup": "^2.48.0",
    "rollup-plugin-dts": "^3.0.1",
    "tslib": "^2.2.0",
    "typedoc": "^0.25.1",
    "typedoc-plugin-markdown": "^3.16.0",
    "typescript": "^4.2.4",
    "vite": "^4.4.9"
  },
  "overrides": {
    "terser": "^5.15.0"
  },
  "ava": {
    "typescript": {
      "rewritePaths": {
        "src/": "test/"
      },
      "compile": false
    },
    "failFast": true,
    "verbose": true
  }
}
